{% extends 'base.jinja2' %}

{% block header %}
    <h1>{% block title %}Лаборатории{% endblock %}</h1>
{% endblock %}

{% block content %}
    <ul class="word">
        {% for lab in labs %}
            <li><a href="{{ url_for('view_lab', lab_id=lab.id) }}" class="link">{{ lab.name }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}