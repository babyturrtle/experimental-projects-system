{% extends 'base.jinja2' %}

{% block header %}
    <h1>{% block title %}Все проекты{% endblock %}</h1>
{% endblock %}

{% block content %}
    <ul class="word">
        {% for project in projects %}
            <li><a href="{{ url_for('view_project', project_id=project.id) }}" class="link">{{ project.name }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}