{% extends 'base.jinja2' %}

{% block header %}
    <h1>{% block title %}Результат поиска{% endblock %}</h1>
{% endblock %}

{% block content %}
    <ul class="word">
        {% for result in results %}
            <li><a href="{{ url_for('view_project', project_id=result.id) }}" class="link">{{ result.name }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}